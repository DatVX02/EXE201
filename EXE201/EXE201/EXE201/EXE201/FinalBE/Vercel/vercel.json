{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "FE/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "FE/dist"
      }
    }
  ],
  "routes": [
    { "src": "^/api/(.*)", "dest": "server.js" },
    { "src": "^(?!/api).*", "dest": "/FE/dist/index.html" }
  ]
}
